<h1>Listing All Articles</h1>
<p>
<%= link_to "Create a new article", new_article_path %>
</p>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  
  <%= @articles.each do |f| %>
    <tr>
      <td><%= f.title %></td>
      <td><%= f.description %></td>
      <td><%= link_to "Edit", edit_article_path(f) %></td>
      <td><%= link_to "Show", article_path(f) %></td>
      <td><%= %></td>
    </tr>
  <% end %>
</table>

<%= link_to "Create Article", new_article_path %>